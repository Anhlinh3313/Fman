@{
    ViewBag.Title = "Quản lý yêu cầu lấy hàng";
    ViewBag.ListRequest = "active";
}
<link href="~/Content/css/confirmDialog.css" rel="stylesheet" />
<!--End Tab manager -->
<div class="content-wrapper scroller" id="style-10">
    <div class="col-md-12">
        <section class="content-header">
            @*<b>
                    Quản lý đơn hàng
                </b>*@
            <ol class="breadcrumb">
                <li><a href="@Url.Action("DashBroad", "DashBroad")"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Quản lý yêu cầu lấy hàng</li>
            </ol>
        </section>
    </div>
    <div class="col-md-12">
        <!--search xác nhận bảng kê gửi bưu cục-->
        <div class="st-form report-revenue message-bb">
            <!-- BEGIN SAMPLE FORM PORTLET-->
            <div class="extend-head st-bg-2">
                <div class="extend-title"><span class="Titleform">Danh sách yêu cầu của bạn</span></div>
            </div>
            <div class="extend-body row">
                <form class="form-horizontal" role="form">
                    <div class="form-body">
                        <div class="row" style="margin-bottom:0px">
                            <div class="col-md-2">
                                <div class="inputField">
                                    <div class="input-group date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
                                        <input type="text" class="inputText width-100 form-starDate" readonly="">
                                        <span class="input-group-btn">
                                            <button class="btn btn-custom inputBut" type="button"><i class="fa fa-calendar"></i></button>
                                        </span>
                                        <span class="label">Từ ngày</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="inputField">
                                    <div class="input-group date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
                                        <input type="text" class="inputText width-100 form-EndDate" readonly="">
                                        <span class="input-group-btn">
                                            <button class="btn btn-custom inputBut" type="button"><i class="fa fa-calendar"></i></button>
                                        </span>
                                        <span class="label">Đến ngày</span>
                                    </div>
                                </div>
                            </div>
                            @*<div class="col-md-3">
                                    <div class="form-group">
                                        <label style="font-size:12px; font-weight:bold; color: #000">Trạng thái đơn hàng</label>
                                        <select class="form-control TypeStatus" data-placeholder="Chọn Trạng Thái"></select>
                                    </div>
                                </div>*@
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>&nbsp;</label>
                                    <div class="input-group">
                                        <input type="button" value="Tìm kiếm" class="btn btn-danger btn-search" style="margin-left:15px;" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="success alert alert-success" style="display:none">
                    </div>
                    <div class="error alert alert-error" style="display:none">
                    </div>
                </form>
            </div>
            <!-- END SAMPLE FORM PORTLET-->
            <!-- BEGIN TABLE PORTLET-->
            <div class="box  listproterler">
                <div class="extend-body row">
                    <div class="TableSearch">
                        @* <a class="btn btn-print custom-lading" title="In vận đơn" style="position:absolute; z-index:10;top:-25px;right:10px;"><i class="fa fa-print"></i><label class=""></label></a>*@
                        <table class="table table-striped table-bordered table-hover" id="listLading">
                            <thead>
                                <tr>
                                    <th class="stt">#</th>
                                    <th class="stt">STT</th>
                                    <th class="code">Mã yêu cầu</th>
                                    <th class="status">Trạng Thái</th>
                                    <th class="sender">Người liên hệ</th>
                                    <th class="sender">Số điện thoại</th>
                                    <th class="sender">Địa chỉ lấy hàng</th>
                                    <th class="createddate">Ngày tạo</th>
                                    <th class="pricemain">Trọng lượng(kg)</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="cancel_request">
            </div>
        </div>
        <!--end search xác nhận bảng kê gửi bưu cục-->
        <!--chỉnh sửa vận đơn-->
    </div>
</div>
<a class="btn btn-default btn-loadmodal display-none" data-toggle="modal" href="#loadmodal">Show hang</a>
<div class="ladingModel modal fade bs-example-modal-lg ladings ladingedit" id="loadmodal" tabindex="-1" role="basic" aria-hidden="true">
    <div class="modal-dialog modal-wide modal-lg st-form report-revenue message-bb">
        <div class="modal-content scroller" id="style-3">
            <div class="modal-header">
                <button type="button" style="color:#f39c28" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Danh sách đơn hàng</h4>
            </div>
            <div class="modal-body">
                <div class="row" id="ladinginfo">
                    <div class="lading-template ">
                        <div class="extend-container col-md-12">
                            <div class="extend-head st-bg-2">
                                <span class="extend-title">
                                    Thông tin yêu cầu <span class="ladingnumber"></span>
                                </span>
                            </div>
                            <div class="extend-body col-md-12">
                                <div class="">
                                    <div class="col-md-12">
                                        <div class="box  listproterler" style="border: 0px;">
                                            <div class="extend-body row" style="background: #f2f2f2;">
                                                <div class="TableSearch">
                                                    <table class="table table-condensed table-bordered table-hover" id="LoadData">
                                                        <thead>
                                                            <tr>
                                                                <th class="stt"><label class="text-center"> <input class="selectAll" type="checkbox" /></label></th>
                                                                <th class="stt">#</th>
                                                                <th class="stt">STT</th>
                                                                <th class="code">Mã VĐ</th>
                                                                <th class="code">Mã tham chiếu</th>
                                                                <th class="status">Trạng Thái VĐ</th>
                                                                <th class="deliveryNote">Lý do giao hàng</th>
                                                                <th class="cod">COD</th>
                                                                <th class="pricemain">Cước phí</th>
                                                                <th class="sender">Người nhận</th>
                                                                <th class="sender">ĐT nhận</th>
                                                                <th class="sender">Địa chỉ tiết</th>
                                                                <th class="sender">Địa chỉ nhận</th>
                                                                <th class="sender">Nội dung hàng hóa</th>
                                                                <th class="sender">Ghi chú</th>
                                                                <th class="status">Hình thức thanh toán</th>
                                                                <th class="createddate">Ngày gửi</th>
                                                                <th class="pricemain">TL(<span id="Weight"></span>)</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody></tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default bt-close-loadmodal" data-dismiss="modal">Đóng</button>
            </div>
            <div style="display:none">
                <div class="bb-barcode" style="width:100px; "></div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!--Bin jquery-->
<script src="~/ScriptSys/ListRequest.js"></script>
<!--End bin jquery-->
