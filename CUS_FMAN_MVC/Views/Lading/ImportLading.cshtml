    <section class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="alert alert-danger message_error" style="display:none">
                </div>
                <div class="message_success alert alert-success" style="display:none">
                    Upload danh sách đơn hàng thành công!
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="col-md-4">
                    <div class="form-group address">
                        <select class="form-control select-choice address-select" data-placeholder="Chọn địa chỉ">
                            <option value="">Chọn địa chỉ</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-1" style="padding-left: 0;padding-right: 5px;">
                    <span class="btn btn-warning btn-social btn-file margin-bottom-15" style="width: 100%;">
                        <i class="fa fa-file-excel-o"></i> Import excel <input id="upload_excel" type="file" accept="application/vnd.ms-excel, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />
                    </span>
                </div>
                <div class="col-md-1" style="padding-left: 0;padding-right: 5px;">
                    <a href="#" class="btn btn-info btn_save margin-bottom-15" style="width: 100%;">
                        <i class="fa fa-plus"></i> Tạo vận đơn
                    </a>
                </div>
                <div class="col-md-1" style="padding-left: 0;">
                    <a href="#" class="btn btn-danger btn_clear margin-bottom-15" style="width: 100%;">
                        <i class="fa fa-times"></i> Hủy
                    </a>
                </div>
                <div class="col-md-4">
                </div>
                <div class="col-md-1" style="padding-left: 0;">
                    <a download href="~/Content/plugins/upload-excel/FileExampleUploadBill.xlsx" class="btn btn-info btn-file margin-bottom-15">
                        <i class="fa fa-download"></i> Tải file mẫu
                    </a>
                </div>
            </div>
            <div class="col-md-12">
                <div id="result"></div>
                <table id="grid-excel" class="data-excel"></table>
            </div>
        </div>
    </section>
    <div class="form-edit-cell">
        <input type="text" id="google_address" class="edit-text" placeholder="" tabindex="1" />
        <input type="button" class="edit-cancel" value="CANCEL" tabindex="2" />
        <input type="button" class="edit-save" value="OK" tabindex="1" />
    </div>
    <div class="edit-triangle"></div>
</div>
