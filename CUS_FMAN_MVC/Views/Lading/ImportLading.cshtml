@{ ViewBag.Title = "Tạo đơn hàng bằng Excel"; ViewBag.ImportActive = "active"; }

<link href="~/Content/plugins/upload-excel/css/infragistics.theme.css" rel="stylesheet" />
<link href="~/Content/plugins/upload-excel/css/infragistics.css" rel="stylesheet" />
<link href="~/Content/plugins/upload-excel/css/styleExcel.css" rel="stylesheet" />
<link href="~/Content/plugins/upload-excel/css/styleExcel.css" rel="stylesheet" />
<link href="~/Content/css/confirmDialog.css" rel="stylesheet" />
<div class="content-wrapper scroller" id="style-10">
    <style>
        #myProgress {
            width: 100%;
            background-color: #ddd;
        }

        #myBar {
            width: 5%;
            height: 30px;
            background-color: #4CAF50;
            text-align: center;
            line-height: 30px;
            color: white;
        }

        .ui-iggrid-scrolldiv tbody {
            font-size: 13px;
        }

        table.data-excel > tbody > tr > td.not_get {
            font-style: initial !important;
        }
        /*tbody > tr:first-child {
            font-style: italic;
        }*/
        tbody > tr {
            font-style: italic;
        }
    </style>
    <section class="content-header">
        <h1>
            Tạo đơn hàng bằng Excel
        </h1>
        @*
            <ol class="breadcrumb">
                <li><a href="@Url.Action(" Index ", "Home ")"><i class="fa fa-dashboard"></i> Home</a></li>
                <li><a href="@Url.Action(" Index ", "Lading ")">Quản lý đơn hàng</a></li>
                <li class="active">Import đơn hàng</li>
            </ol>*@
    </section>
