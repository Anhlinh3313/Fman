<body class="hold-transition skin-blue sidebar-mini" @*style="overflow:hidden;"*@>
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="/" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><img src="" alt="" height="50" id="Favicon-Company-Home" class="img-responsive"></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg">
                    <img src="" alt="" height="50" class="img-responsive" id="Logo-Company">
                </span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <div class="rows">
                    <div class="col-md-7" style="padding-left: 0;">
                        <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                            <span class="sr-only">Toggle navigation</span>
                        </a>
                    </div>
                    <div class="col-md-5">
                        <a href="/" class="logo-reponsive">
                            <img src="" alt="Logo" height="30" class="img-responsive" />
                        </a>
                        <div>
                            <!-- search form -->
                            <div class="sidebar-formB">
                                <div class="input-group">
                                    <input type="text" name="code" class="form-control trackingNumber" placeholder="Mã đơn hàng">
                                    <span class="input-group-btn">
                                        <button type="button" name="search" style="height:28px" id="search-btn" class="btn btn-flat btn-tracking">
                                            <i class="fa fa-search"></i>
                                        </button>
                                    </span>
                                </div>
                            </div>
                            <!-- /.search form -->
                        </div>
                    </div>
                </div>
            </nav>
        </header>
