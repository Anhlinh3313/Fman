            <section class="sidebar">
                <!-- Sidebar user panel -->
                @Html.Action("CustomerPartial", "Home")
                <ul class="sidebar-menu">
                    <li class="@ViewBag.DashBroadActive treeview">
                        <a href="@Url.Action("DashBroad", "DashBroad")">
                            <i class="fa fa-tachometer"></i> <span> DashBroad</span>
                        </a>
                    </li>
                    <li class="@ViewBag.CreateRequestActive treeview">
                        <a href="@Url.Action("CreateRequest", "Lading")">
                            <i class="fa fa-plus"></i> <span> Tạo yêu cầu nhận hàng</span>
                        </a>
                    </li>

                    <li class="@ViewBag.ListRequest treeview">
                        <a href="@Url.Action("RequestList", "Lading")">
                            <i class="fa fa-list"></i> <span> Quản lý yêu cầu</span>
                        </a>
                    </li>
                    <li class="@ViewBag.CreateActive treeview">
                        <a href="@Url.Action("AddLading", "Lading")">
                            <i class="fa fa-plus"></i> <span> Tạo đơn hàng</span>
                        </a>
                    </li>
                    <li class="@ViewBag.ImportActive treeview">
                        <a href="@Url.Action("ImportLading", "Lading")">
                            <i class="fa fa-file-excel-o"></i> <span> Tạo đơn hàng Excel</span>
                        </a>
                    </li>
                    @*<li class="@ViewBag.WaitingPickup treeview">
