
@{
    ViewBag.Title = "Tiền thu hộ";
    ViewBag.Money = "active";
}
<style>
    .fa {
        font-size: inherit;
    }
</style>
<div class="content-wrapper scroller" id="style-10">
    <div class="col-lg-12">
        <section class="content-header">
            <h1>
                Tiền thu hộ
            </h1>
            <ol class="breadcrumb">
                <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Tiền thu hộ</li>
            </ol>
        </section>
    </div>
    <section class="content">
        <div class="st-form">
            <div class="row">
                <div class="col-md-2">
                    <div class="inputField active">
                        <div class="input-group date" data-provide="datepicker" data-date-format="mm-dd-yyyy">
                            <input type="text" class="inputText search-from-date" readonly>
                            <span class="input-group-btn">
                                <button class="btn btn-custom inputBut" type="button"><i class="fa fa-calendar"></i></button>
                            </span>
                            <span class="label">Từ ngày (mm/dd/yyyy)</span>
                        </div>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="inputField active">
                        <div class="input-group date" data-provide="datepicker" data-date-format="mm-dd-yyyy">
                            <input type="text" class="inputText search-to-date" readonly>
                            <span class="input-group-btn">
                                <button class="btn btn-custom inputBut" type="button"><i class="fa fa-calendar"></i></button>
                            </span>
                            <span class="label">Đến ngày (mm/dd/yyyy)</span>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <button class="btn btn-danger btn-search margin-top-10">Tìm kiếm</button>
                </div>
            </div>
        </div>
        <div class="row sumary-price">
            <div class="col-md-4 col-sm-12 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-aqua"><i class="fa fa-shopping-cart"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Đơn hàng đang giao</span>
                        <span class="info-box-number delivering"></span>
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-sm-12 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-blue"><i class="fa fa-calendar-check-o"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Đơn hàng đã giao (chưa thanh toán)</span>
                        <span class="info-box-number delive_iHubServicered"></span>
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-sm-12 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-yellow"><i class="fa fa-exchange"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Đơn hàng đã hoàn</span>
                        <span class="info-box-number return"></span>
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-sm-12 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-blue-active"><i class="fa fa-dollar "></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Đơn hàng đã thanh toán</span>
                        <span class="info-box-number deliveredPayment"></span>
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-sm-12 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-red-active"><i class="fa fa-recycle"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Đơn hàng đã hủy</span>
                        <span class="info-box-number cancel" ></span>
                    </div>
                </div>
            </div>
            @*<div class="col-md-4 col-sm-12 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-green-active"><i class="fa fa-money"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Tổng doanh thu</span>
                        <span class="info-box-number summaryAll"></span>
                    </div>
                </div>
            </div>*@
        </div>
    </section>
    <section class="content">
        <!-- Nav tabs -->
        <ul class="nav nav-tabs tab-flat" role="tablist">
            <li role="presentation" class="active"><a href="#detail" class="change-tab" typeCOD="1" role="tab" data-toggle="tab">Đơn hàng đang giao <span class="badge bg-aqua graded count-delivering">0</span></a></li>
            <li role="presentation"><a href="#detail" class="change-tab" typeCOD="2" role="tab" data-toggle="tab">Đơn hàng đã giao (chưa thanh toán)<span class="badge bg-blue graded count-delive_iHubServicered">0</span></a></li>
            <li role="presentation"><a href="#detail" class="change-tab" typeCOD="3" role="tab" data-toggle="tab">Đơn hàng đã hoàn <span class="badge bg-yellow graded count-return">0</span></a></li>
            <li role="presentation"><a href="#detail" class="change-tab" typeCOD="4" role="tab" data-toggle="tab">Đơn hàng đã thanh toán <span class="badge bg-blue-active graded count-deliveredPayment">0</span></a></li>
            <li role="presentation"><a href="#detail" class="change-tab" typeCOD="5" role="tab" data-toggle="tab">Đơn hàng đã hủy <span class="badge bg-red graded count-cancel">0</span></a></li>
            @*<li role="presentation"><a href="#detail" class="change-tab" typeCOD="0" role="tab" data-toggle="tab">Tổng doanh thu <span class="badge bg-green-active graded count-summaryAll" >0</span></a></li>*@
        </ul>
        <!-- Tab panes -->
        <div class="tab-content tabs-flat">
            <div role="tabpanel" class="tab-pane active" id="detail">
                <div class="ctl-show-data">
                    <div class="dataTables_wrapper form-inline no-footer pageajax">
                        <div class="table-scrollable">
                            <table class="table table-striped table-bordered table-hover" id="report" role="grid">
                                <thead>
                                    <tr>
                                        <th>Tùy chọn</th>
                                        <th>STT</th>
                                        <th>Mã vận đơn</th>
                                        <th>Thời gian gửi</th>
                                        <th>Trạng thái</th>
                                        <th>Dịch vụ</th>
                                        <th>Người nhận</th>
                                        <th>Điện thoại nhận</th>
                                        <th>Địa chỉ nhận</th>
                                        <th>Trọng lượng (kg)</th>
                                        <th>Thu hộ (COD)</th>
                                        <th>Cước phí vận chuyển</th>
                                        <th>Hình thức thanh toán</th>
                                    </tr>
                                </thead>                                
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
<script src="/ScriptSys/PriceListScript.js"></script>
