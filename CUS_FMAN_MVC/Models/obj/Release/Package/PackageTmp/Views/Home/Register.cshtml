<!-- BEGIN BODY -->
<body class="hold-transition login-page">
    <style>
        .display-none {
            display: none !important;
        }
    </style>
    <div class="login-box">
        <div class="login-logo div-logo">
            <a href="/"><img src="~/Content/img/tasetco.png" style="margin:0 auto" height="50" id="Logo-Company" class="img-responsive logo"></a>
        </div>
        <!-- /.login-logo -->
        <div class="login-box-body">
            <p class="login-box-msg">ĐĂNG KÝ THÀNH VIÊN</p>
            <div>
                <div class="form-group has-feedback">
                    <input type="email" class="email form-control" name="Email" placeholder="Email đăng nhập">
                    <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    <input type="password" class="password form-control" name="PassWord" placeholder="Mật khẩu">
                    <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    <input type="password" class="confirmPassword form-control" name="ConfirmPassword" placeholder="Xác nhận mật khẩu">
                    <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    <input type="text" class="name form-control" name="CustomerName" placeholder="Họ và tên">
                    <span class="fa fa-user-secret form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    <input type="text" class="phone form-control" name="Phone" placeholder="Điện thoại">
                    <span class="fa fa-phone form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    <input type="text" class="form-control address" id="address" name="Address" placeholder="Địa chỉ">
                    <input type="hidden" class="form-control cityid" id="cityId" name="CityId">
                    <input type="hidden" class="form-control districtid" id="districtId" name="DistrictId">
                    <input type="hidden" class="form-control wardid" id="wardid" name="WardId">
                    <input type="hidden" class="form-control lat" id="lat" name="Lat">
                    <input type="hidden" class="form-control lng" id="lng" name="Lng">
                    <span class="fa fa-map-pin form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    <label class="font-red-haze lblmessage display-none" style="color:red"><i class="fa fa-exclamation-triangle"></i> <span class="message">Dữ liệu không hợp lệ!</span></label>
                </div>
                <div class="row">
                    <div class="col-xs-6">
                        <a href="/dang-nhap.html" class="btn btn-warning"> <i class="fa fa-arrow-left"></i> Đăng nhập</a>
                    </div>
                    <div class="col-xs-6">
                        <button class="btn btn-success btn-block btn-flat btn-register"> <i class="fa fa-user"></i> Đăng ký</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.login-box-body -->
    </div>
    <!-- /.login-box -->
    <!-- jQuery 2.2.3 -->
    <script src="/content/plugins/jQuery/jquery-2.2.3.min.js"></script>
    <!-- Bootstrap 3.3.6 -->
    <script src="/content/bootstrap/js/bootstrap.min.js"></script>
    <!-- iCheck -->
    <script src="/content/plugins/iCheck/icheck.min.js"></script>
    <!--Custom-->
    <script src="/Tool_JS/js/Enviroment.js"></script>
    <script src="/Tool_JS/js/ToolFunction.js"></script>
    @* Test *@
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCkR-wl8rYwyb5XhvinbcfZOuFsRV2IFDo&sensor=false&amp;libraries=places"></script>

    @* ================================= *@
    <script src="~/ScriptSys/GetData/title.js"></script>
    <script src="~/ScriptSys/GetData/GetData.js"></script>

    @*<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCfqPJqJnthPCSdgAs4pn1Gcc5RZrElg3U&sensor=false&amp;libraries=places"></script>*@
    @*online*@
