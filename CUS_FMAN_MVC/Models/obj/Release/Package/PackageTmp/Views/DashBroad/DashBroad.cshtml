    </section>
</div>
<a class="btn btn-default btn-loadmodal display-none" data-toggle="modal" href="#loadmodal">Show hang</a>

<div class="ladingModel modal fade bs-example-modal-lg ladings ladingedit" id="loadmodal" tabindex="-1" role="basic" aria-hidden="true">
    <form id="ladingFrom ">
        <div class="modal-dialog modal-wide modal-lg st-form report-revenue message-bb">
            <div class="modal-content scroller" id="style-3">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                    <h4 class="modal-title">Quản lý đơn hàng - <b id="nameModal">Mới tạo</b></h4>
                </div>
                <div class="modal-body">
                    <div class="col-md-12">
                        <div id="sender" class="row">
                            <div class="extend-container col-md-12">
                                <div class="extend-head st-bg-2">
                                    <span class="extend-title">Tìm kiếm đơn hàng</span>
                                </div>
                                <div class="extend-body col-md-12">
                                    <div class="col-md-2">
                                        <div class="inputField">
                                            <div class="input-group date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
                                                <input type="text" class="inputText width-100 form-starDate-loadmodal" readonly="">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-custom inputBut" type="button"><i class="fa fa-calendar"></i></button>
                                                </span>
                                                <span class="label">Từ ngày</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="inputField">
                                            <div class="input-group date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
                                                <input type="text" class="inputText width-100 form-EndDate-loadmodal" readonly="">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-custom inputBut" type="button"><i class="fa fa-calendar"></i></button>
                                                </span>
                                                <span class="label">Đến ngày</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>&nbsp;</label>
                                            <div class="input-group">
                                                <input type="button" value="Tìm kiếm" class="btn btn-danger btn-search-loadmodal" style="margin-left:15px;" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row" id="ladinginfo">
                            <div class="lading-template ">
                                <div class="extend-container col-md-12">
                                    <div class="extend-head st-bg-2">
                                        <span class="extend-title">
                                            Thông tin đơn hàng <span class="ladingnumber"></span>
                                        </span>
                                    </div>
                                    <div class="extend-body col-md-12">
                                        <div class="">
                                            <div class="col-md-12">
                                                <div class="box  listproterler" style="border: 0px;">
                                                    <div class="extend-body row" style="background: #f2f2f2;">
                                                        <div class="TableSearch">
                                                            <table class="table table-striped table-bordered table-hover" id="LoadData">
                                                                <thead>
                                                                    <tr>
                                                                        <th class="stt">#</th>
                                                                        <th class="stt">STT</th>
                                                                        <th class="code">Mã yêu cầu</th>
                                                                        <th class="status">Trạng Thái</th>
                                                                        <th class="sender">Người liên hệ</th>
                                                                        <th class="sender">Số điện thoại</th>
                                                                        <th class="sender">Địa chỉ nhận hàng</th>
                                                                        <th class="createddate">Ngày tạo</th>
                                                                        <th class="pricemain">Trọng lượng(kg)</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody></tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default bt-close-loadmodal" data-dismiss="modal">Đóng</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
    </form>
    <!-- /.modal-dialog -->
</div>
