
@{
    ViewBag.Title = "Tạo đơn hàng";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.CreateActive = "active";
}
<style>
    .st-form .row {
        margin-bottom: 5px;
    }

    .st-form .multiselect-container {
        padding-left: 30px;
    }
</style>
<link href="~/Content/plugins/jquery-validation/validationEngine.jquery.css" rel="stylesheet" />
<div class="content-wrapper scroller" id="style-10">
    <div class="col-md-12">
        <section class="content-header">
            <h1>
                Tạo đơn hàng
            </h1>
            <ol class="breadcrumb">
                <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Tạo đơn hàng</li>
            </ol>
        </section>
        <div class="st-form ladings">
            <form id="ladingFrom" action="#">
                <div class="success alert alert-success" style="display:none">
                </div>
                <div id="sender" class="row" data-district-id="" data-city-id="">
                    <div class="extend-container col-md-12">
                        <div class="extend-head st-bg-2">
                            <div class="extend-title">
                                Thông tin người gửi
                                @*<div class="col-md-1 pull-right">
                                        <div class="form-horizontal">
                                            <div class="form-group">
                                                <div class="input-icon">
                                                    <i class="fa fa-clock-o"></i>
                                                    <input type="text" class="form-control timepicker timepicker-default">
                                                </div>
                                            </div>
                                        </div>
                                    </div>*@
                                @*<div class="col-md-2 pull-right">
                                        <div class="input-group">
                                            <div class="input-group date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
                                                <input type="text" id="datetimepicker1" style="height:22px !important; border-radius:0px !important; background-color: rgba(255, 255, 255, 0)" class="form-control width-100 form-starDate" readonly="">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-custom inputBut" style="line-height:10px; margin-top:0px;padding:3px 5px; color:#222222" type="button"><i class="fa fa-calendar"></i></button>
                                                </span>
                                            </div>
                                        </div>
                                    </div>*@
                            </div>
                        </div>
                        <div class="extend-body col-md-12">
                            <div class="col-md-4 padding-right-0">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label><i class="fa fa-user"></i> Họ Tên</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control sender-name validate[required]" value="">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 padding-right-0">
                                    <div class="form-group">
                                        <label><i class="fa fa-phone"></i> Điện thoại</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control sender-phone validate[required]" value="">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-5">
                                <div class="form-group">
                                    <label><i class="fa fa-map-marker"></i> Địa chỉ </label>
                                    <div class="input-group">
                                        <input type="text" class="form-control sender-address validate[required]" id="sender-address" value="">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><i class="fa fa-building"></i> Công ty</label>
                                    <div class="input-group">
                                        <input type="text" class="form-control sender-company" value="">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row" id="ladinginfo">
                    <div class="lading-template " style="display:none">
                        <div class="extend-container col-md-12">
                            <div class="extend-head st-bg-2">
                                <span class="extend-title">
                                    Đơn hàng <span class="ladingnumber"></span>
                                    <button class="remove-lading" style="height: 31px;width: 30px;background-color:red;position:absolute;right: 15px;border: 0;top: 0;">x</button>
                            </div>
                            <div class="extend-body col-md-12">
                                <div class="">
                                    <div class="col-md-4 padding-right-0">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label><i class="fa fa-user"></i> Tên người nhận</label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control receiver-name validate[required]">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6 padding-right-0">
                                            <div class="form-group">
                                                <label><i class="fa fa-phone"></i> Số điện thoại nhận</label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control receiver-phone validate[required]">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-8 padding-right-0">
                                        <div class="col-md-8" style="padding-left:0px">
                                            <div class="form-group">
                                                <label><i class="fa fa-sticky-note-o"></i> Địa chỉ chi tiết</label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control receiver-address-note" placeholder="Khu/Khối, Lô/Block">
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <div class="">
                                    <div class="col-md-12"></div>
                                    <div class="col-md-4 padding-right-0">
                                        <div class="form-group">
                                            <label><i class="fa fa-map-marker"></i> Địa chỉ nhận <i>(tên đường, quận huyện)</i></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control receiver-address validate[required]">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-8 padding-right-0">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label style="width:100%"><i class="fa fa-credit-card"></i> Tỉnh thành</label>
                                                <select name="province" id="province" class="form-control select-choice chosen-select province" data-placeholder="Chọn tỉnh thành" placeholder="Vui lòng chọn Tỉnh/Thành" required>
                                                    <option value="">Chọn tỉnh thành</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label style="width:100%"><i class="fa fa-credit-card"></i> Quận huyện</label>
                                                <select name="district" id="district" class="form-control select-choice chosen-select district" data-placeholder="Chọn quận huyện" placeholder="Vui lòng chọn Quận/Huyện" required>
                                                    <option value="">Chọn quận huyện</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label style="width:100%"><i class="fa fa-credit-card"></i> Phường xã</label>
                                                <select name="ward" id="ward" class="form-control select-choice chosen-select ward" data-placeholder="Chọn phường xã" placeholder="Vui lòng chọn Phường/Xã" required>
                                                    <option value="">Chọn phường xã</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="">
                                    <div class="col-md-12"></div>
                                    <div class="col-md-4 padding-right-0">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label><i class="fa fa-balance-scale"></i> TL(kg)</label>
                                                <div class="input-group">
                                                    <input type="number" class="form-control weight validate[required, min[0.00001]">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3 padding-right-0">
                                            <div class="form-group">
                                                <label><i class="fa fa-dropbox"></i> Số kiện</label>
                                                <div class="input-group">
                                                    <input type="number" value="1" class="form-control  number validate[required, min[0.00001]]">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-5 padding-right-0">
                                            <div class="form-group">
                                                <label style="width:100%"><i class="fa fa-cog"></i> Dịch vụ</label>
                                                <select class="form-control select-choice service" data-placeholder="Chọn dịch vụ">
                                                    <option value="">Chọn dịch vụ</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label style="width:100%"><i class="fa fa-plus-circle"></i> Dịch vụ gia tăng <a style="color:red" class="pull-right display remove-all" title="Bỏ chọn"><i class="fa fa-window-close"></i></a></label>
                                                <select class="btn btn-default servicedvgt" data-placeholder="Chọn DVGT" multiple="multiple"
                                                        data-label="left" itemprop="2" itemscope="2" data-select-all="true" data-width="100%" data-filter="true" data-action-onchange="true"></select>
                                            </div>
                                        </div>
                                        <div class="col-md-8 padding-right-0" style="padding-left: 5px">
                                            <div class="form-group">
                                                <label><i class="fa fa-file-text-o"></i> Nội dung hàng hóa</label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control description">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    @*<div class="col-md-3">
                                            <div class="form-group">
                                                <label><i class="fa fa-file-text-o"></i> Ghi chú</label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control noted">
                                                </div>
                                            </div>
                                        </div>*@
                                </div>
                                <div class="col-md-12 padding-right-0">
                                    <div class="">
                                        <div class="col-md-4 insured-div  padding-right-0" style="display:none">
                                            <div class="">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label><i class="fa fa-money"></i> Tiền khai giá</label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control insured validate[required, min[0.00001]]">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label><i class="fa fa-money"></i> Cước bảo hiểm</label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control insured-price" readonly>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 cod-div  padding-right-0" style="display:block">
                                            <div class="">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label><i class="fa fa-money"></i> Thu hộ (COD)</label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control cod validate[required, min[0.00001]]">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label><i class="fa fa-money"></i> Cước thu hộ(COD)</label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control cod-price" readonly>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 pack-div  padding-right-0" style="display:none">
                                            <div class="">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label><i class="fa fa-dropbox"></i> Hình thức đóng gói</label>
                                                        <select class="form-control select-choice pack-type " data-placeholder="Chọn đóng gói">
                                                            <option value="">Đóng gói</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label><i class="fa fa-money"></i> Phí đóng gói</label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control pack-price" readonly>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="">
                                    <div class="col-md-2 display-none">
                                        <div class="form-group">
                                            <label>Dài (cm)</label>
                                            <div class="input-group">
                                                <input type="number" value="0" class="form-control  length">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 display-none">
                                        <div class="form-group">
                                            <label>Rộng (cm)</label>
                                            <div class="input-group">
                                                <input type="number" value="0" class="form-control  width">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 display-none">
                                        <div class="form-group">
                                            <label>Cao (cm)</label>
                                            <div class="input-group">
                                                <input type="number" value="0" class="form-control height">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 display-none">
                                        <div class="form-group">
                                            <label>Khối lượng (kg)</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control mass" readonly>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="check-hide-price">
                                    <div class="col-md-2 pull-left">
                                        <div class="form-group">
                                            <label style="width:100%"><i class="fa fa-credit-card"></i> Thanh toán <a title="Chi tiết cước phí" class="fa fa-info-circle pull-right detail-price"></a></label>
                                            <select class="form-control select-choice payment validate[required]" data-placeholder="Chọn Thanh toán">
                                                <option value="">Thanh toán</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-2 hide-price display-none">
                                        <div class="form-group">
                                            <label><i class="fa fa-money"></i> Cước chính</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control price-main" readonly>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 hide-price display-none">
                                        <div class="form-group">
                                            <label><i class="fa fa-money"></i> Phụ phí xăng dầu</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control ppxd-price" readonly>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 hide-price display-none">
                                        <div class="form-group">
                                            <label><i class="fa fa-money"></i> Phụ phí VSVX</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control price-far" readonly>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 hide-price display-none">
                                        <div class="form-group">
                                            <label><i class="fa fa-money"></i> Tổng phí DVGT</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control dvgt-price" readonly>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 display-none">
                                        <div class="form-group">
                                            <label><i class="fa fa-money"></i> Phụ phí khác</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control other-price" readonly>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label><i class="fa fa-money"></i> Tổng chi phí <span style="color:red">( Tạm tính )</span></label>
                                            <div class="input-group">
                                                <input type="text" class="form-control amount" readonly>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 display-none">
                                        <div class="form-group">
                                            <label><i class="fa fa-money"></i> VAT 10%</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control vat" readonly>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="lading-list">

                    </div>
                </div>
                <div class="alert alert-danger error" style="display:none">
                </div>
                <div class="col-md-12">
                    <button class="add-more btn btn-default" style="margin-right:10px" id="add-more">Thêm</button>
                    <button class="add-lading btn btn-info" type="submit" style="margin-right:10px" id="add-lading">Tạo đơn hàng</button>
                </div>
            </form>
        </div>
    </div>
</div>

<script src="~/Content/plugins/jquery-validation/jquery.validationEngine.js"></script>
<script src="~/Content/plugins/jquery-validation/jquery.validationEngine.Custom.js"></script>
<script src="~/Content/plugins/jquery-validation/jquery.validationEngine-vi.js"></script>
<script src="/ScriptSys/AddLadingsScript.js"></script>


